<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProteinePlus.MVVM.Views.NavPage"
             Title="NavPage"
             BackgroundColor="AliceBlue">

    <VerticalStackLayout>
        <Label Text="Navigation Page Content"
               VerticalOptions="Center"
               HorizontalOptions="Center"
               Margin="10" />
        <Button
            x:Name="closeButton" Text="Close" Margin="20" Clicked="closeButton_Clicked"/>
    </VerticalStackLayout>
</ContentPage>